  <!-- ==================== HERO SECTION ==================== -->
  <section class="hero" id="accueil">
    <!-- Background -->
    <div class="hero-bg">
      <img src="/img/paa.jpeg" alt="Forêt tropicale">
      <div class="hero-overlay"></div>
    </div>

    <!-- Decorations -->
    <div class="hero-decoration hero-decoration-1"></div>
    <div class="hero-decoration hero-decoration-2"></div>

    <!-- Content -->
