        </a>
        <a href="/connexion" class="btn btn-outline">Accéder à GESTREB</a>
      </div>
    </div>

    <!-- Scroll Indicator -->
    <div class="hero-scroll">
      <div class="hero-scroll-indicator">
        <div class="hero-scroll-dot"></div>
      </div>
    </div>
  </section>

